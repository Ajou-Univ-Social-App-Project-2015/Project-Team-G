<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    <FrameLayout
        android:id="@+id/sss"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#696969"
        android:visibility="invisible">
        <LinearLayout
            android:layout_width="500dp"
            android:layout_height="300dp"
            android:orientation="vertical"
            android:background="#6495ED"
            android:textColor="#ffffffff"
            android:layout_gravity="center"

           >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="60dp"
                android:textSize="15pt"
                android:text="로그아웃 하시겠습니까?"
                android:layout_gravity="center"
                android:layout_marginTop="50dp"
                android:textStyle="bold"
                android:background="#6495ED"
                android:textColor="#ffffffff"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:layout_gravity="center"
                android:orientation="horizontal"
                android:layout_marginTop="65dp"
                android:background="#6495ED"
                android:textColor="#ffffffff">
                <Button
                    android:id="@+id/yes"
                    android:layout_width="250dp"
                    android:layout_height="100dp"
                    android:text="예"
                    android:textStyle="bold"
                    android:textSize="15pt"
                    android:background="#6495ED"
                    android:textColor="#ffffffff"/>
                <Button
                    android:id="@+id/no"
                    android:layout_width="250dp"
                    android:layout_height="100dp"
                    android:text="아니오"
                    android:textStyle="bold"
                    android:textSize="15pt"
                    android:background="#6495ED"
                    android:textColor="#ffffffff"/>


            </LinearLayout>

        </LinearLayout>

    </FrameLayout>
    <FrameLayout 
        android:id="@+id/frame"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="visible"
        >
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="#ffffffff"
        >
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:orientation="horizontal"
            android:background="#6495ED"
            >
            <Button
                android:id="@+id/logout2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1.3"                
                android:text="로그아웃"
                android:textSize="10pt"
                android:background="#6495ED"
                android:textColor="#ffffffff"
                ></Button>
            <ImageView
               android:layout_width="match_parent"
               android:layout_height="70dp"
               android:layout_weight="1"
               android:layout_gravity="center"
                android:layout_marginTop="7dp"
                android:layout_marginBottom="10dp"
               android:src="@drawable/logologo"

               android:background="#6495ED"
                ></ImageView>
            <Button
                android:id="@+id/write"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1.3"             
                android:text="기록하기"
                android:textSize="10pt"
                android:background="#6495ED"
                android:textColor="#ffffffff"
                ></Button>
        </LinearLayout>
        <TextView
            android:id="@+id/myName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="45dp"
            android:text="진동우님, 환영합니다!"
            android:textColor="#696969"
            android:textSize="10pt"
            android:gravity="center"
            />
        <LinearLayout
            android:layout_width="250dp"
            android:layout_height="60dp"
            android:layout_marginTop="65dp"
            android:layout_gravity="center"
            android:background="#6495ED">
        <Button 
            android:id="@+id/care"
            android:layout_width="246dp"
            android:layout_height="56dp"
            android:background="#ffffffff"
            android:text="건강 관리"
            android:textSize="10pt"
           android:layout_margin="2dp"

            />
        </LinearLayout>
        <LinearLayout
            android:layout_width="250dp"
            android:layout_height="60dp"
            android:layout_marginTop="15dp"
            android:layout_gravity="center"
            android:background="#6495ED">
        <Button
            android:id="@+id/sns"
            android:layout_width="246dp"
            android:layout_height="56dp"
            android:background="#ffffffff"
            android:text="SNS"
            android:textSize="10pt"
            android:layout_margin="2dp"

            />
        </LinearLayout>
        <LinearLayout
            android:layout_width="250dp"
            android:layout_height="60dp"
            android:layout_marginTop="15dp"
            android:layout_gravity="center"
            android:background="#6495ED">
        <Button 
            android:id="@+id/log"
            android:layout_width="246dp"
            android:layout_height="56dp"
            android:background="#ffffffff"
            android:text="최근 기록"
            android:textSize="10pt"
            android:layout_margin="2dp"
            />



        </LinearLayout>

        <TextView
            android:id="@+id/loading"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="Loading.."
            android:textStyle="bold"
            android:textColor="#000000"
            android:textSize="25pt"
            android:layout_gravity="center"
            android:visibility="invisible"/>
        
        </LinearLayout>
        </FrameLayout>
        
    
        <FrameLayout 
    android:id="@+id/writew"
    android:layout_width="167dp"
    android:layout_height="262dp"
    android:layout_marginTop="100dp"
    android:background="#ffffffff"
            android:layout_marginLeft="432dp"
    android:visibility="invisible"
    >
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:background="#00ff0000"
        >
  
        <LinearLayout
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:orientation="vertical"
             android:layout_margin="2dp"
            >
            <Button 
                android:id="@+id/cpwnd"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center"
                android:text="체중"
                android:textSize="8pt"
                android:background="#6495ED"
                android:textColor="#ffffffff"
                />
            <Button 
                android:id="@+id/dnsehdfid"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center"
                android:text="운동량"
                android:textSize="8pt"
                android:background="#6495ED"
                android:textColor="#ffffffff"
                android:layout_marginTop="2dp"
                />
            <Button 
                android:id="@+id/dmatlr"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center"
                android:text="음식"
                android:textSize="8pt"
                android:background="#6495ED"
                android:textColor="#ffffffff"
                android:layout_marginTop="2dp"
                />
            <Button 
                android:id="@+id/gufekd"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center"
                android:text="혈당"
                android:textSize="8pt"
                android:background="#6495ED"
                android:textColor="#ffffffff"
                android:layout_marginTop="2dp"
                />
            <Button 
                android:id="@+id/gufdkq"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:gravity="center"
                android:text="혈압"
                android:textSize="8pt"
                android:background="#6495ED"
                android:textColor="#ffffffff"
                android:layout_marginTop="2dp"
                />
            
        </LinearLayout>
        ></LinearLayout>
    
</FrameLayout>
    
</RelativeLayout>
